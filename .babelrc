{
  "compact": false,
  "presets": [
    "@babel/env",
    [
      "@babel/typescript", {
        "onlyRemoveTypeImports": true
      }
    ]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/syntax-dynamic-import",
    "@babel/plugin-transform-runtime"
  ]
}
